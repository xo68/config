[
    {
        "key": "cmd+'",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "!terminalFocus"
    },
    {
        "key": "cmd+'",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+left",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "cmd+right",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "cmd+up",
        "command": "editor.action.fontZoomIn"
    },
    {
        "key": "cmd+down",
        "command": "editor.action.fontZoomOut"
    }
]
